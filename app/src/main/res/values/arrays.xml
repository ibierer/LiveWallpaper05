    <string-array name="graph_options">
        <item name="Two Toroids">1/((sqrt(x^2 + y^2) - 1.6 + cos(t))^2 + (z - sin(t))^2) + 1/((sqrt(x^2 + y^2) - 1.6 - cos(t))^2 + (z + sin(t))^2) = 4</item>
        <item name="Three Toroids">1/((sqrt(x^2 + y^2) - 1.6 + sin(t))^2 + (z + cos(t))^2) + 1/((sqrt(x^2 + y^2) - 1.6 + sin(t + 2π/3))^2 + (z + cos(t + 2π/3))^2) + 1/((sqrt(x^2 + y^2) - 1.5 + sin(t + 4π/3))^2 + (z + cos(t + 4π/3))^2) = 5</item>
        <item name="Four Toroids">1/((sqrt(x^2 + y^2) - 1.6 + sin(t))^2 + (z + cos(t))^2) + 1/((sqrt(x^2 + y^2) - 1.6 + sin(t + π/2))^2 + (z + cos(t + π/2))^2) + 1/((sqrt(x^2 + y^2) - 1.6 + sin(t + π))^2 + (z + cos(t + π))^2) + 1/((sqrt(x^2 + y^2) - 1.6 + sin(t + 3π/2))^2 + (z + cos(t + 3π/2))^2) = 7.3</item>
        <item name="Saturn">1/(x^2 + y^2 + z^2) + 1/((1.2*sqrt(x^2 + y^2) - 2.7)^20 + (20z)^20) = 1</item>
        <item name="Spherocylinder">4 = x^2 + z^2 + ((abs(y - 1.15) - abs(y + 1.15))/2 + y)^2</item>
        <item name="Expanding Concentric Spheres">sin(sqrt(x^2 + y^2 + z^2) - t) = 0</item>
        <item name="Heart">(0.5x)^2 + (abs(0.5x) - 0.5y)^2 = 1</item>
        <item name="Spiral">sqrt((x - sin(z - t))^2 + (y - cos(z - t))^2) = 1</item>
        <item name="Ripples">-z = -cos(3sqrt(x^2 + y^2) - t + sin(3sqrt(x^2 + y^2) - t + sin(3sqrt(x^2 + y^2) - t + sin(3sqrt(x^2 + y^2) - t)/2)/2)/2)/(3sqrt(x^2 + y^2 + z^2))</item>
        <item name="Circle">x^2 + y^2 = 4</item>
        <item name="Square">1 - (0.5x)^100 = 0.5abs(y)</item>
        <item name="Octagon">(x/3)^40 + (y/3)^40 + (z/1.5)^40 + (((x - y)/6)^2 + ((y - x)/6)^2)^20 + (((-x - y)/6)^2 + ((x + y)/6)^2)^20 = 0.99</item>
        <item name="Sphere">x^2 + y^2 + z^2 = 4</item>
        <item name="Cylinder">(0.5x)^2 + (0.5y)^40 + (0.5z)^2 = 1</item>
        <item name="Orbit">0.2/((x + 2.5sin(t))^2 + (y + 2.5cos(t))^2 + z^2) + 1/((x - 0.5sin(t))^2 + (y - 0.5cos(t))^2 + z^2) = 1</item>
        <item name="Arch">1/sqrt((sqrt(x^2 + z^2) - 2.5)^2 + y^2) - z = 1</item>
        <item name="Dunking Doughnut">3/sqrt((sqrt(x^2 + (z - 6sin(t))^2) - 3)^2 + y^2) - z = 3</item>
        <item name="Ring around Arch">1/sqrt((sqrt(x^2 + z^2) - 2.8)^2 + y^2) - z + 1/sqrt((sqrt(y^2 + (z - 3)^2) - 1.5)^2 + x^2) = 1</item>
        <item name="Chain">1/sqrt((sqrt((x + 2.05 + sin(t)/6)^2 + y^2) - 1.5)^2 + z^2) + 1/sqrt((sqrt(x^2 + z^2) - 1.5)^2 + y^2) + 1/sqrt((sqrt((x - 2.05 - sin(t)/6)^2 + y^2) - 1.5)^2 + z^2) = 6</item>
        <item name="Pythagorean's Theorem">x^2 + y^2 = z^2</item>
        <item name="Two Cubes">1/(x^40 + y^40 + z^40) + 1/((x + 4sin(t))^40 + (y + 4cos(t))^40 + z^40) = 1</item>
        <item name="Hole Thru Cube">1/((abs(0.3x) + 0.25)^20 + (abs(0.3y) + 0.25)^20 + (abs(0.3z) + 0.25)^20) - 1/((x)^2 + (y)^2)^100 = 1</item>
        <item name="Guitar">1/(x^2 + (.75(y + 1.2))^4 + (3z)^40) - .025/((x - .7)^2 + (y + .9)^2) - .025/((x + .7)^2 + (y + .9)^2) - .06/((x - .9)^2 + (y + .85)^2) - .06/((x + .9)^2 + (y + .85)^2) - 1/((4x)^2 + (4(y + .6))^2 + (10(z - .25))^10)^10 - 1/((2x)^2 + (2(y + .6))^2 + (5z)^40)^10 - 1/((1.5x)^2 + (1.4(y + 1.6))^2 + (5z)^40)^10 + 1/((4x)^2 + (y - 1)^40 + (10(z - .255))^2)^10 + 1/((4x/(y - 1))^40 + (3(y - 2.2))^40 + (10(z + .2y - .6))^10)^40 = 1</item>
        <item name="Skateboard">1/(80(y + 2)^2 + 80z^2 + x^10)^10 + 1/(80(y - 2)^2 + 80z^2 + x^10)^10 + 1/(8(y + 2)^2 + 8z^2 + 10000000000000000000000000(x - 0.75)^40)^10 + 1/(8(y + 2)^2 + 8z^2 + 10000000000000000000000000(x + 0.75)^40)^10 + 1/(8(y - 2)^2 + 8z^2 + 10000000000000000000000000(x - 0.75)^40)^10 + 1/(8(y - 2)^2 + 8z^2 + 10000000000000000000000000(x + 0.75)^40)^10 + 1/(10(y - 2)^2 + 10(z - 0.25)^2 + 20x^2)^2 + 1/(10(y + 2)^2 + 10(z - 0.25)^2 + 20x^2)^2 + 1/(0.00002y^10 + 10000000000(z - 0.5 - (0.25y)^4)^10 + x^2)^10 = 1</item>
        <item name="Miscellaneous 1">sqrt(sin(x)^2 + sin(y)^2 + sin(z)^2) - 0.5sin(t) = 1</item>
        <item name="Miscellaneous 2">sin(x) + sin(y) + sin(z) = 0</item>
